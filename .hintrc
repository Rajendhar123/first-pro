{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ],
    "axe/name-role-value": "off",
    "disown-opener": "off",
    "axe/structure": "off",
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]"
        ]
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "axe/forms": "off",
    "button-type": "off",
    "axe/keyboard": "off"
  }
}